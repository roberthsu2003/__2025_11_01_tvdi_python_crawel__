   "source": [
    "import time \n",
    "\n",
    "def main_sync():\n",
    "    start_time = time.time()\n",
    "    time.sleep(2)\n",
    "    end_time = time.time()\n",
    "    print(f\"花費的時間: {(end_time - start_time):.2f} 秒\")\n",
    "\n",
    "if __name__ == \"__main__\":\n",
    "    main_sync()\n"
   ]
  }
